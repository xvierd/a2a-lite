{
  "name": "a2a-lite",
  "version": "0.2.5",
  "description": "Build A2A agents in 8 lines. Add enterprise features when you need them.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src tests",
    "typecheck": "tsc --noEmit",
    "example:hello": "npx tsx examples/01-hello-world.ts"
  },
  "keywords": [
    "a2a",
    "agent",
    "agent-to-agent",
    "ai",
    "llm",
    "sdk"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@a2a-js/sdk": "^0.3.10",
    "uuid": "^11.1.0"
  },
  "peerDependencies": {
    "express": "^4.21.2 || ^5.1.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": false
    }
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.0.0",
    "@types/uuid": "^10.0.0",
    "express": "^5.1.0",
    "tsup": "^8.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.5.0",
    "vitest": "^2.0.0",
    "supertest": "^7.0.0",
    "@types/supertest": "^6.0.0"
  }
}
